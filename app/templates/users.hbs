<div class="col-md-12">

    <h2>Users</h2>

    <table>
        <thead>
        <th>ID</th>
        <th>Username</th>
        <th>Email</th>
        </thead>
        <tbody>
        {{#each user in model}}
            <tr>
                <td>{{user.id}}</td>
                <td>{{user.username}}</td>
                <td>{{user.email}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>

</div>
