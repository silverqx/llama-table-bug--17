<table>
    <thead>
      <th>ID</th>
      <th>Message</th>
      <th>Version</th>
    </thead>
    <tbody>
      {{#each note in model}}
        <tr>
            <td>{{note.id}}</td>
            <td>{{note.message}}</td>
            <td>{{note.version}}</td>
        </tr>
      {{/each}}
    </tbody>
</table>
